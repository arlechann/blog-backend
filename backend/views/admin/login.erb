<!DOCTYPE html>
<html>
  <head>
    <title>管理者ログイン</title>
  </head>
  <body>
    <h1>管理者ログイン</h1>
    <form method="POST">
      <div>
        <%= message %>
      <div>
      <div>
        <label for="email">メールアドレス</label>
        <input type="email" id="email" name="email" value="<%= email %>" />
      </div>
      <div>
        <label for="password">パスワード</label>
        <input type="password" id="password" name="password" />
      </div>
      <div>
        <input type="submit" value="ログイン" />
      </div>
    </form>
  </body>
</html>

